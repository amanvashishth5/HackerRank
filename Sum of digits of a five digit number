#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n,sum=0,rem;
    scanf("%d", &n);
    rem=n%10;
    n=n/10;
    sum+=rem;  //copy that 5 times
    
    rem=n%10;
    n=n/10;
    sum+=rem;

    rem=n%10;
    n=n/10;
    sum+=rem;

    rem=n%10;
    n=n/10;
    sum+=rem;

    rem=n%10;
    n=n/10;
    sum+=rem;
    printf("%d",sum);
    return 0;
}
